{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "run-p api web",
    "web": "next dev",
    "api": "cd packages/api && bun dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy && cd packages/api && bun deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "postinstall": "bun gen:env",
    "gen:env": "bun ./.github/scripts/env.ts",
    "gen:db": "cd packages/api && bun generate",
    "clean": "rm -rf .turbo node_modules bun.lockb packages/api/node_modules packages/api/bun.lockb"
  },
  "dependencies": {
    "@api": "workspace:*",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.12.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^34.3.0",
    "wrangler": "^3.63.1"
  }
}
